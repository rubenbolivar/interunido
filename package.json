{
	"name": "interunido",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "NODE_ENV=development vite dev",
		"build": "NODE_ENV=production prisma generate && vite build",
		"preview": "vite preview",
		"check": "NODE_ENV=development svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "NODE_ENV=development svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"postinstall": "prisma generate"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.3",
		"@sveltejs/adapter-vercel": "^5.5.0",
		"@sveltejs/kit": "^2.15.1",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@types/bcryptjs": "^2.4.6",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/node": "^22.10.2",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"esm-env": "^1.2.1",
		"globals": "^15.0.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^5.16.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.4.11"
	},
	"dependencies": {
		"@prisma/client": "^4.16.2",
		"@types/chart.js": "^2.9.41",
		"bcryptjs": "^2.4.3",
		"chart.js": "^4.4.7",
		"jsonwebtoken": "^9.0.2",
		"prisma": "^4.16.2"
	}
}
